{
    "name": "yiisoft/yii-swagger",
    "type": "library",
    "description": "Open Api Swagger 3 for Yii Framework",
    "keywords": [
        "yii",
        "swagger",
        "api"
    ],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii-swagger/issues?state=open",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii-swagger"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "nyholm/psr7": "^1.3",
        "swagger-api/swagger-ui": "^3.34",
        "yiisoft/aliases": "^1.0",
        "yiisoft/assets": "^1.0@dev",
        "yiisoft/cache": "^3.0@dev",
        "yiisoft/csrf": "^3.0@dev",
        "yiisoft/data-response": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/event-dispatcher": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "yiisoft/yii-view": "^3.0@dev",
        "zircote/swagger-php": "^3.1"
    },
    "require-dev": {
        "infection/infection": "^0.16.3",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.15"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Swagger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Swagger\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
